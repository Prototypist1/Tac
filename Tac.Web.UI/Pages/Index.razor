@page "/"
@using Tac.Emit.Runner;
@using Tac.Backend.Emit.Walkers;
@using Tac.Model;
@using Tac.Model.Elements;
@using Tac.Model.Instantiated;

<div id="flex">
    <textarea @bind="Model.InputValue" id="code">
    </textarea>
    <input type="button" @onclick="Model.Execute" disabled="@Model.Running" value="@Model.Text" id="run" />
    <div id="console">
        @foreach (var item in Model.Output)
        {
            <div>@item</div>
        }
    </div>
</div>
@functions {
    Test Model = new Test();
}
